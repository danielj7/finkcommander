/* Abbreviated day and month names, day, year, time */
"%a, %b %e, %Y  %I:%M %p" = "%a, %e %b %Y  %H:%M Uhr";

/* Full month name, day, year, time */
"%A, %B %e, %Y  %I:%M %p" = "%A, %e %B, %Y  %H:%M Uhr";

/* Numeric format with four-digit year */
"%m/%d/%Y" = "%d.%m.%Y";

/* Numeric format with two-digit year */
"%m/%d/%y" = "%d.%m.%y";

/* Numeric format with four-digit year plus time */
"%m/%d/%Y  %I:%M %p" = "%d.%m.%Y  %H:%M Uhr";

/* Numeric format with two-digit year plus time */
"%m/%d/%y  %I:%M %p" = "%d.%m.%y  %H:%M Uhr";

/* Minimum width for numeric format with two-digit year plus time */
"130.0" = "130.0";

/* Minimum width for numeric format with two-digit year plus time */
"145.0" = "145.0";

/* Minimum width for abbreviated day and month names, day, year, time */
"170.0" = "170.0";

/* Minimum width for full month name, day, year, time */
"240.0" = "240.0";

/* Minimum width for numeric format with four-digit year */
"80.0" = "80.0";

/* Default column title */
"Date" = "Datum";

